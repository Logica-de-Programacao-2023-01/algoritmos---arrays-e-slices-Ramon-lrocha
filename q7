package main

import "fmt"

func main() {
	slice := make([]int, 0, 5)

	var number int
	fmt.Print("Digite um nÃºmero inteiro: ")
	fmt.Scan(&number)

	found := false

	for _, num := range slice {
		if num == number {
			found = true
			break
		}
	}

	if !found {
		slice = append(slice, number)
	}

	fmt.Println("Slice Resultante:", slice)
}
